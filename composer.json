{
    "name": "lokal-base-theme/wordpress-website",
    "description": "Wordpress Website",
    "repositories": {

        "gravityforms": {
            "type": "package",
            "package": {
                "name": "gravityforms/gravityforms",
                "type": "wordpress-plugin",
                "version": "2.7.13.1",
                "dist": {
                    "type": "zip",
                    "url": "https://www.dropbox.com/scl/fi/1ihuzgsj9v52ge8g0y5ot/gravityforms_2.7.13.1.zip?rlkey=z03z7l5y5pdmbj2hhqeme12ov&raw=1"
                }
            }
        },
        "facetwp": {
            "type": "package",
            "package": {
                "name": "facetwp/facetwp",
                "type": "wordpress-plugin",
                "version": "4.2.5",
                "dist": {
                    "type": "zip",
                    "url": "https://www.dropbox.com/scl/fi/mrfh689ciqdyj7pta7g9v/facetwp-4.2.5.zip?rlkey=plhl5pk4sh5x9bjg5whcn5kbo&raw=1",
                    "reference": "master"
                }
            }
        },
        "better-search-replace-pro": {
            "type": "package",
            "package": {
                "name": "deliciousbrains-plugin/better-search-replace-pro",
                "type": "wordpress-plugin",
                "version": "1.4.2",
                "dist": {
                    "type": "zip",
                    "url": "https://www.dropbox.com/s/8lqtxqorc9ky3s8/better-search-replace.1.4.2.zip?raw=1",
                    "reference": "master"
                }
            }
        },
        "wpackagist": {
            "type": "composer",
            "url": "https://wpackagist.org"
        },
        "delicious-brains": {
            "type": "composer",
            "url": "https://composer.deliciousbrains.com"
        }
    },
    "require": {
        "wpackagist-plugin/health-endpoint": "^1.0",
        "wpackagist-plugin/akismet":"^5.0",
        "gravityforms/gravityforms": "^2.0",
        "facetwp/facetwp": "^4.0",
        "wpackagist-plugin/redirection": "^5.0",
        "wpackagist-plugin/enable-media-replace": "^4.0",
        "wpackagist-plugin/auto-advance-for-gravity-forms":"^4.0",
        "wpackagist-plugin/query-monitor": "^3.0",
        "deliciousbrains-plugin/better-search-replace-pro":"^1.0",
        "wpackagist-plugin/imagify" : "^2.0"
       
    },
    "require-dev": {
        "wpackagist-plugin/health-endpoint": "^1.0",
        "wpackagist-plugin/akismet":"^5.0",
        "gravityforms/gravityforms": "^2.0",
        "facetwp/facetwp": "^4.0",
        "wpackagist-plugin/redirection": "^5.0",
        "wpackagist-plugin/enable-media-replace": "^4.0",
        "wpackagist-plugin/auto-advance-for-gravity-forms":"^4.0",
        "wpackagist-plugin/query-monitor": "^3.0",
        "deliciousbrains-plugin/better-search-replace-pro":"^1.0",
        "wpackagist-plugin/imagify" : "^2.0"
    },
    "autoload": {
        "psr-0": {
            "Acme": "src/"
        }
    },
    "extra": {
        "installer-types": [
            "wordpress-plugin",
            "wordpress-dropin"
        ],
        "installer-paths": {
            "../../plugins/{$name}": [
                "type:wordpress-plugin",
                "type:wordpress-dropin"
            ]
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
